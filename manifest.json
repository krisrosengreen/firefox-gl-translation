{
  "manifest_version": 2,
  "name": "GL Translation",
  "version": "1.0",
  "permissions": [
    "*://*.nutserut.gl/*"
  ],
  "description": "Translates single greenlandic words with translator",
  "content_scripts": [
    {
      "matches" : ["*://*.knr.gl/kl/*", "*://*.sermitsiaq.ag/kl/*"],
      "js": ["translator.js"],
      "css": ["overlay.css"]
    }
  ]
}

